
@{
    ViewData["Title"] = "List";
}

<h1>List</h1>

<h1>Product List</h1>

<table class=" table table-bordered">
    <tr>
        <th>Product Id</th>
        <th>Product Name</th>
        <th>Product Price</th>
    </tr>
@foreach (Product p in (IEnumerable<Product>)ViewData["productlist"])
{ 
<tr>
    <td>@p.Id</td>
    <td>@p.Name</td>
    <td>@p.Price</td>
</tr>
}
</table>